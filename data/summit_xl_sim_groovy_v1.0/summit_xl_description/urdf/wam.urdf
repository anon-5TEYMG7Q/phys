<?xml version="1.0"?>
<robot name="wam" xmlns:xacro="http://www.ros.org/wiki/xacro"> 

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find summit_xl_description)/urdf/wam.gazebo" />

  <!-- Constants for robot dimensions -->
  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="joint_damping" value="100.0" />
  <xacro:property name="joint_friction" value="0.0" />
  <xacro:property name="joint_effort_limit" value="30.0" />
  <xacro:property name="joint_velocity_limit" value="2.0" />
  <xacro:property name="mechanical_reduction" value="1.0" />
  
  <!-- Used for fixing robot to Gazebo 'base_link' -->
  <link name="world"/>
  <joint name="fwf" type="fixed">
    <parent link="world"/>
    <child link="base_footprint"/>
  </joint>

  <!-- BASE FOOTPRINT -->
  <!-- base_footprint is a fictitious link(frame) that is on the ground right below base_link origin,
             navigation stack depends on this frame -->
  <link name="base_footprint">
      <visual>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <geometry>
             <box size="0.001 0.001 0.001" />
          </geometry>
      </visual>

      <collision>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <geometry>
            <box size="0.001 0.001 0.001" />
         </geometry>
     </collision>
  </link>

  <!-- BASE_LINK (RED) -->
  <link name="base_link">
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" />
      <inertia  ixx="5.0" ixy="0.0"  ixz="0.0"  iyy="5.0"  iyz="0.0"  izz="5.0" />
    </inertial>
    <visual>
       <origin xyz="0 0 0" rpy="0 0 0" />
       <geometry>
	 <mesh filename="package://robotnik_wam_sim/mesh/wambase.stl"/>
       </geometry>
    </visual>
    <collision>
       <origin xyz="0 0 0" rpy="0 0 0 " />
       <geometry>
         <mesh filename="package://robotnik_wam_sim/mesh/wambase.stl"/>
       </geometry>
    </collision>     
  </link>

  <joint name="base_footprint_joint" type="fixed">
     <!-- NB While you would think this would make it go up, it is oddly reversed.
      This moves the joint to 0,0,0 where we want it for rviz, etc. -->
     <!-- origin xyz="0 0 0.050" rpy="0 0 0" / -->
     <origin xyz="0 0 0.5" rpy="0 0 0" />
     <parent link="base_footprint"/>
     <child link="base_link"/>
  </joint>  

  <!-- WAM ARM -->
  <link name="wam_link1">
    <inertial>
      <mass value="2.3936"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link1_visual">
        <mesh filename="package://robotnik_wam_sim/mesh/wam1.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.75 0.75 0.75 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link1_collision">
        <mesh filename="package://robotnik_wam_sim/mesh/wam1.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <joint name="j1_joint" type="revolute">
    <parent link="base_link"/>
    <child link="wam_link1"/>
    <!-- origin xyz="0.125 0 0.55" rpy="0 0 0" / -->
    <origin xyz="0.0 0 0.0" rpy="0 0 0" />
    <axis xyz="0 0 1"/>
    <limit effort="${joint_effort_limit}" lower="-2.6" upper="2.6" velocity="${joint_velocity_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
  </joint>
  
  <transmission name="j1_transmission">
	<type>transmission_interface/SimpleTransmission</type>
    <joint name="j1_joint"/>
    <actuator name="j1">
	  <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
      <motorTorqueConstant>1</motorTorqueConstant>
    </actuator>
  </transmission>

  <link name="wam_link2">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.8487"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link2_visual">
        <mesh filename="package://robotnik_wam_sim/mesh/wam2.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.75 0.75 0.75 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link2_collision">
        <mesh filename="package://robotnik_wam_sim/mesh/wam2.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <joint name="j2_joint" type="revolute">
    <parent link="wam_link1"/>
    <child link="wam_link2"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 1 0"/>
    <limit effort="${joint_effort_limit}" lower="-2.0" upper="2.0" velocity="${joint_velocity_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
  </joint>


  <transmission name="j2_transmission">
	<type>transmission_interface/SimpleTransmission</type>
    <joint name="j2_joint"/>
    <actuator name="j2">
	  <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
      <motorTorqueConstant>1</motorTorqueConstant>
    </actuator>
  </transmission>

  <link name="wam_link3">
    <inertial>
      <mass value="0.43"/>
      <origin rpy="0 0 0" xyz="0 0 0.225"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link3_visual">
        <mesh filename="package://robotnik_wam_sim/mesh/wam3.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.75 0.75 0.75 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link3_collision">
        <mesh filename="package://robotnik_wam_sim/mesh/wam3.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <joint name="j3_joint" type="revolute">
    <parent link="wam_link2"/>
    <child link="wam_link3"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="${joint_effort_limit}" lower="-2.8" upper="2.8" velocity="${joint_velocity_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
  </joint>
 
  <transmission name="j3_transmission">
	<type>transmission_interface/SimpleTransmission</type>
    <joint name="j3_joint"/>
    <actuator name="j3">
	  <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
      <motorTorqueConstant>1</motorTorqueConstant>
    </actuator>
  </transmission>


  <link name="wam_link4">
    <inertial>
      <mass value="0.52"/>
      <origin rpy="0 0 0" xyz="-0.045 -0.15 0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link4_visual">
        <mesh filename="package://robotnik_wam_sim/mesh/wam4.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.75 0.75 0.75 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link4_collision">
        <mesh filename="package://robotnik_wam_sim/mesh/wam4.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <joint name="j4_joint" type="revolute">
    <parent link="wam_link3"/>
    <child link="wam_link4"/>
    <origin rpy="-1.57079632679 0 0" xyz="0.045 0.0 0.55"/>
    <axis xyz="0 0 1"/>
    <limit effort="${joint_effort_limit}" lower="-0.9" upper="2.8" velocity="${joint_velocity_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
  </joint>

  <transmission name="j4_transmission">
	<type>transmission_interface/SimpleTransmission</type>
    <joint name="j4_joint"/>
    <actuator name="j4">
	  <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
      <motorTorqueConstant>1</motorTorqueConstant>
    </actuator>
  </transmission>

  <link name="wam_link5">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.076675"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link5_visual">
        <mesh filename="package://robotnik_wam_sim/mesh/wam5.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.75 0.75 0.75 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link5_collision">
        <mesh filename="package://robotnik_wam_sim/mesh/wam5.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <joint name="j5_joint" type="revolute">
    <parent link="wam_link4"/>
    <child link="wam_link5"/>
    <origin rpy="1.57079632679 0 0" xyz="-0.045 -0.3 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="${joint_effort_limit}" lower="-4.8" upper="1.3" velocity="${joint_velocity_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
  </joint>
  
  <transmission name="j5_transmission">
	<type>transmission_interface/SimpleTransmission</type>
    <joint name="j5_joint"/>
    <actuator name="j5">
	  <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
      <motorTorqueConstant>1</motorTorqueConstant>
    </actuator>
  </transmission>

  <link name="wam_link6">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.10695"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link6_visual">
        <mesh filename="package://robotnik_wam_sim/mesh/wam6.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.75 0.75 0.75 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link6_collision">
        <mesh filename="package://robotnik_wam_sim/mesh/wam6.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <joint name="j6_joint" type="revolute">
    <parent link="wam_link5"/>
    <child link="wam_link6"/>
    <origin rpy="-1.57079632679 0 0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="${joint_effort_limit}" lower="-1.6" upper="1.6" velocity="${joint_velocity_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
  </joint>
  
  <transmission name="j6_transmission">
	<type>transmission_interface/SimpleTransmission</type>
    <joint name="j6_joint"/>
    <actuator name="j6">
	  <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
      <motorTorqueConstant>1</motorTorqueConstant>
    </actuator>
  </transmission>

  
  <link name="wam_link7">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.013925"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link7_visual">
        <mesh filename="package://robotnik_wam_sim/mesh/wam7.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.75 0.75 0.75 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link7_collision">
        <mesh filename="package://robotnik_wam_sim/mesh/wam7.stl" scale="1.0 1.0 1.0"/>
        <!--                <box size="0.10 0.10 0.10"/> -->
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <joint name="j7_joint" type="revolute">
    <parent link="wam_link6"/>
    <child link="wam_link7"/>
    <origin rpy="1.57079632679 0 0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="${joint_effort_limit}" lower="-2.2" upper="2.2" velocity="${joint_velocity_limit}"/>
    <!--  <safety_controller k_velocity="0.5"/>-->
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
  </joint>

  <transmission name="j7_transmission">
	<type>transmission_interface/SimpleTransmission</type>
    <joint name="j7_joint"/>
    <actuator name="j7">
	  <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
      <motorTorqueConstant>1</motorTorqueConstant>
    </actuator>
  </transmission>


  <link name="wam_tcp"/>
  <joint name="wamtcp_fix" type="fixed">
    <parent link="wam_link7"/>
    <child link="wam_tcp"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.2"/>
  </joint>

  <!-- GRIPPER -->
  <!-- joint name="wam_gripper_joint" type="fixed">
    <origin rpy="0 0 1.57075" xyz="0 0 0.065"/>
    <parent link="wam_link7"/>
    <child link="gripper_base_link"/>
  </joint -->
      
</robot>

